<application>
  <component name="ParameterNameHintsSettings">
    <blacklists>
      <blacklist language="TypeScript" />
      <blacklist language="Java" />
      <blacklist language="Groovy" />
      <blacklist language="Kotlin">
        <added pattern="org.springframework.messaging.simp.SimpMessagingTemplate.convertAndSend(destination,payload)" />
      </blacklist>
    </blacklists>
    <option id="SHOW_PROPERTY_HINT" value="true" />
    <option id="java.build.like.method" value="false" />
    <option id="SHOW_LOCAL_VARIABLE_HINT" value="true" />
    <option id="java.simple.sequentially.numbered" value="false" />
    <option id="java.method.name.contains.parameter.name" value="false" />
  </component>
</application>