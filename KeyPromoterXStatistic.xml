<application>
  <component name="KeyPromoterXStatistic">
    <option name="statistics">
      <Statistic Action="Debug 'All Tests'">
        <Item>
          <IdeaActionID>DebugClass</IdeaActionID>
          <ShortCut>'⌃⇧D'</ShortCut>
          <Description>Debug 'All Tests'</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
      <Statistic Action="Declaration">
        <Item>
          <IdeaActionID>GotoDeclaration</IdeaActionID>
          <ShortCut>'⌘B'</ShortCut>
          <Description>Declaration</Description>
          <Count>2</Count>
        </Item>
      </Statistic>
      <Statistic Action="Find Usages">
        <Item>
          <IdeaActionID>FindUsages</IdeaActionID>
          <ShortCut>'⌥F7'</ShortCut>
          <Description>Find Usages</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
      <Statistic Action="Generate...">
        <Item>
          <IdeaActionID>Generate</IdeaActionID>
          <ShortCut>'⌘N' or '⌃⏎'</ShortCut>
          <Description>Generate...</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
      <Statistic Action="Project">
        <Item>
          <IdeaActionID>ActivateProjectToolWindow</IdeaActionID>
          <ShortCut>'⌘1'</ShortCut>
          <Description>Project</Description>
          <Count>2</Count>
        </Item>
      </Statistic>
      <Statistic Action="TODO">
        <Item>
          <IdeaActionID>ActivateTODOToolWindow</IdeaActionID>
          <ShortCut>'⌘6'</ShortCut>
          <Description>TODO</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
      <Statistic Action="Terminal">
        <Item>
          <IdeaActionID>ActivateTerminalToolWindow</IdeaActionID>
          <ShortCut>'⌥F12'</ShortCut>
          <Description>Terminal</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
      <Statistic Action="Toggle Line Breakpoint">
        <Item>
          <IdeaActionID>ToggleLineBreakpoint</IdeaActionID>
          <ShortCut>'⌘F8'</ShortCut>
          <Description>Toggle Line Breakpoint</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
    </option>
  </component>
</application>